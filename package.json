{
  "name": "generate-data-schema",
  "displayName": "generate-data-schema",
  "description": "",
  "publisher": "yiri",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bugszhou/generate-data-schema"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:generate-data-schema.data2schema"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.data2schema",
        "title": "data2schema"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.data2schema",
          "group": "1_modification"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "generate data schema",
      "properties": {
        "generate-data-schema.default": {
          "type": "object",
          "default": {
            "number": -1,
            "string": "",
            "boolean": false,
            "array": [],
            "object": {}
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash.camelcase": "^4.3.0"
  }
}
